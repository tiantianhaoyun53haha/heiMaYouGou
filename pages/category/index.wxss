.cate_container {
  /* 
    高度 等于 视口的高度 - 头部搜索框的高度
    calc  100vh 
    不要less来编译 calc中的代码！！
     */
  /* 原样输出 */
  height: calc(100vh - 90rpx);
  display: flex;
}
.cate_container .left_menu {
  /* 
    子项 高度默认等于父项的高度
     */
  flex: 1;
}
.cate_container .left_menu .menu_item {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 30rpx;
}
.cate_container .left_menu .active {
  color: var(--themeColor);
  border-left: 5rpx solid currentColor;
}
.cate_container .right_menu {
  flex: 3;
}
.cate_container .right_menu .good_group .goods_title {
  height: 80rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cate_container .right_menu .good_group .goods_title .delimiter {
  color: #ccc;
}
.cate_container .right_menu .good_group .goods_title .goods_name {
  color: #000;
  font-size: 30rpx;
  padding: 0 10rpx;
}
.cate_container .right_menu .good_group .goods_list {
  display: flex;
  flex-wrap: wrap;
}
.cate_container .right_menu .good_group .goods_list navigator {
  width: 33.33%;
  text-align: center;
}
.cate_container .right_menu .good_group .goods_list navigator image {
  width: 50%;
}
